    "font-awesome": "^4.7.0",
    "jquery": "^3.2.1",
    "jquery-match-height": "^0.7.2",
    "jquery-sticky": "^1.0.4",
    "laravel-mix": "^1.7.2",
    "malihu-custom-scrollbar-plugin": "^3.1.5",
    "owl.carousel": "^2.2.0",
    "popper": "^1.0.1",
    "popper.js": "^1.12.9"
  },
  "devDependencies": {
    "block-ui": "^2.70.1",
